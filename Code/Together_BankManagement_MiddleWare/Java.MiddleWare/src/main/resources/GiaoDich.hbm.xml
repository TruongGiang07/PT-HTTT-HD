<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class catalog="QL_NGAN_HANG" name="bankmgmt.POJO.GiaoDich" table="GiaoDich" schema="dbo">
        <id name="maGD" type="integer">
            <column name="MaGiaoDich"/>
            <generator class="identity"/>
        </id>
        <property name="soTienGD" type="double">
            <column name="SoTienGiaoDich"/>
        </property>
        <property name="ngayGD" type="date">
            <column name="NgayGiaoDich"/>
        </property>
        <property name="maKhachHang" type="integer">
            <column name="MaKHGiaoDich"/>
        </property>
        <property name="maNhanVien" type="integer">
            <column name="MaGDVien"/>
        </property>
        <property name="loaiGD" type="integer">
            <column name="LoaiGD"/>
        </property>
    </class>
</hibernate-mapping>
